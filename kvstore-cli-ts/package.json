{
  "name": "@zio-raft/kvstore-cli",
  "version": "0.1.0",
  "description": "KVStore CLI - interact with a distributed key-value store",
  "main": "dist/index.js",
  "bin": {
    "kvstore": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e:new": "vitest --run tests/e2e/server-not-running.test.ts tests/e2e/server-managed.test.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:tests": "find tests -type f \\( -name '*.js' -o -name '*.d.ts' -o -name '*.map' \\) -delete",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kvstore",
    "cli",
    "zio-raft",
    "distributed"
  ],
  "author": "ZIO Raft Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@zio-raft/typescript-client": "file:../typescript-client",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^8.56.0",
    "prettier": "^3.8.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
